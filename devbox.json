{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.12.0/.schema/devbox.schema.json",
  "packages": [
    "poetry@1.8.3",
    "ngrok@3.9.0",
    "python@3.11.5",
    "gcc@12.2.0",
    "stdenv.cc.cc.lib",
  ],
  "shell": {
    "init_hook": [
      // Resolves libstdc++ issue
      // https://github.com/jetify-com/devbox/issues/1596#issuecomment-2009296660
      "export LD_LIBRARY_PATH=./.devbox/nix/profile/default/lib",
      "poetry install",
    ],
    "scripts": {
      "test": [
        "echo \"Error: no test specified\" && exit 1",
      ],
    },
  },
}